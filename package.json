{
  "name": "angular-bare-bones",
  "version": "0.0.0",
  "main": "electron-app/main.js",
  "scripts": {
    "ng": "ng",
    "electron-start-UI-hosted-server": "electron ./electron-app/main-load-hosted-ui-server\"",
    "electron-start-UI-local-server": "concurrently \"ng serve\" \"electron ./electron-app/main-load-local-ui-server\"",
    "electron-start-UI-local": "electron .",
    "electron-build-start-UI-local": "ng build --prod --base-href ./ && electron .",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@amcharts/amcharts5": "5.2.12",
    "@amcharts/amcharts5-fonts": "5.0.1",
    "@angular/animations": "~13.0.0",
    "@angular/cdk": "^13.2.2",
    "@angular/common": "~13.0.0",
    "@angular/compiler": "~13.0.0",
    "@angular/core": "~13.0.0",
    "@angular/forms": "~13.0.0",
    "@angular/material": "^13.2.2",
    "@angular/platform-browser": "~13.0.0",
    "@angular/platform-browser-dynamic": "~13.0.0",
    "@angular/router": "~13.0.0",
    "@types/dotenv": "8.2.0",
    "core-js": "^2.5.7",
    "d3": "^7.4.4",
    "dotenv": "15.0.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.2",
    "@angular/cli": "~13.0.2",
    "@angular/compiler-cli": "~13.0.0",
    "@angular/language-service": "~13.0.0",
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@types/d3": "^7.4.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "concurrently": "^7.3.0",
    "electron": "^16.0.0",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.4.3"
  }
}
